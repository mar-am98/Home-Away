// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init


datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl      = env("DIRECT-URL")
}

generator client {
  provider = "prisma-client-js"
}

model property {
  id String @id @default(uuid())
  name String
  tagLine String
  description String
  category String
  price Int
  image String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  clerckId String
  location String
}
